    modifier prankOwner(RayFi rayFi, bool isPrank) {
        if (isPrank) {
            vm.startPrank(rayFi.owner());
        }
        _;
        if (isPrank) {
            vm.stopPrank();
        }
    }

    function airdropRayFi(RayFi rayFi, bool isPrank) public prankOwner(rayFi, isPrank) {
        GasliteDrop gasliteDrop = new GasliteDrop();
        rayFi.setIsExcludedFromRewards(address(gasliteDrop), true);
        rayFi.setIsFeeExempt(address(gasliteDrop), true);
        rayFi.approve(address(gasliteDrop), TOTAL_AMOUNT);
        gasliteDrop.airdropERC20(address(rayFi), s_addresses, s_amounts, TOTAL_AMOUNT);
    }

    function run() external {
        address mostRecentDeployedRayFi = DevOpsTools.get_most_recent_deployment("RayFi", block.chainid);
        vm.startBroadcast();
        airdropRayFi(RayFi(mostRecentDeployedRayFi), false);
        vm.stopBroadcast();
    }

